                @Html.DisplayFor(modelItem => item.farmId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.declaredBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.declaredDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "AddEditAnimal", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "DeleteAnimal", new { id = item.id })
            </td>
        </tr>
}
    </tbody>
</table>
