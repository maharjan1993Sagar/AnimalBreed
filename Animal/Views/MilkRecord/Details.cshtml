        <h3 class="m-1 font-weight-bold center text-primary">Milk Record</h3>
    </div>

    <div style="padding-top:10px;">
        <table class="table table-bordered" id="myTable">
            <tr>
                <th>
                   Id
                </th>
                <td>
                    @Html.DisplayFor(model => model.id)
                </td>
            </tr>
            <tr>
                <th>
                   Ear Tag Number
                </th>
                <td>
                    @Html.DisplayFor(model => model.earTagNumber)
                </td>
            </tr>
            <tr>
                <th>
                   Milking Status
                </th>
                <td>
                    @Html.DisplayFor(model => model.milkingStatus)
                </td>
            </tr>
            <tr>
                <th>
                  Recording Period
                </th>
                <td>
                    @Html.DisplayFor(model => model.recordingPeriod)
                </td>
            </tr>
            <tr>
                <th>
                   Recording Date
                </th>
                <td>
                    @Html.DisplayFor(model => model.recordingDate)
                </td>
            </tr>
            <tr>
                <th>
                   Milk Volume
                </th>
                <td>
                    @Html.DisplayFor(model => model.milkVolume)
                </td>
            </tr>
            <tr>
                <th>
                   Milk Sample Box No
                </th>
                <td>
                    @Html.DisplayFor(model => model.milkSampleBoxNo)
                </td>
            </tr>
            <tr>
                <th>
                  Lab Id
                </th>
                <td>
                    @Html.DisplayFor(model => model.labId)
