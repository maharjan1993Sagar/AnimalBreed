            <th>
                @Html.DisplayNameFor(model => model.milkingStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.createdAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.updatedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ownerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.keeperId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.farmId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.declaredBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.declaredDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.earTagNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.weight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.speciesId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.breedId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dob)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sireId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.damId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.noOfCalving)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lastCalvingDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pregnencyStatus)
            </td>
