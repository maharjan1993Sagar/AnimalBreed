@model IEnumerable<Animal.Models.AnimalRegistration>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="AddEditAnimal">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.earTagNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.weight)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.speciesId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.breedId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dob)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sireId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.damId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.noOfCalving)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lastCalvingDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pregnencyStatus)
