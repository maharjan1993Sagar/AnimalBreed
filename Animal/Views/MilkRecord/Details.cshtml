            </tr>
            <tr>
                <th>
                    Fat Percentage Id
                </th>
                <td>
                    @Html.DisplayFor(model => model.fatPercentage)
                </td>
            </tr>

            @*<th>
                    @Html.DisplayNameFor(model => model.snf)
                </th>
